{
  "name": "MyBAN",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Rachit Dua"
  },
  "scripts": {
    "preinstall": "node scripts/preinstall-script.js",
    "setup": "yarn install && yarn pod-install",
    "pod-install": "npx pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "yarn lint:eslint && yarn lint:ts",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "lint:fix": "eslint . --fix",
    "clear:watchman": "watchman watch-del . ; watchman watch-project .",
    "prestart": "yarn clear:watchman",
    "prestart:reset": "yarn prestart",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "svg": "yarn svg:transform && yarn svg:generate",
    "svg:generate": "node scripts/generate-svgs.js",
    "svg:transform": "node scripts/transform-svgs.js",
    "clean": "yarn clean:android && yarn clean:ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "web:build:production": "rm -rf server/dist/ && webpack --mode=production --config webpack.config.js",
    "web": "webpack serve -d inline-source-map --mode=development --config webpack.config.js",
    "server": "node ./server/app.js",
    "deploy:web": "yarn web:build:production && cd server && gcloud app deploy"
  },
  "engines": {
    "node": ">=16.18.1",
    "yarn": ">=1.22.19"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@reduxjs/toolkit": "^1.9.1",
    "eslint-plugin-local-rules": "^1.3.2",
    "iban": "^0.0.14",
    "invert-color": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.0",
    "react-native-flex-layout": "^0.1.5",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screen-brightness": "^2.0.0-alpha",
    "react-native-screens": "^3.18.2",
    "react-native-shadow-2": "^7.0.6",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.7.0",
    "react-native-web": "^0.18.10",
    "react-redux": "^8.0.5",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@svgr/webpack": "^6.5.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/iban": "^0.0.32",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.18.10",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.7",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "workspaces": [
    "server"
  ]
}
